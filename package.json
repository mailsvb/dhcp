{
  "name": "dhcpd.js",
  "version": "1.0.0",
  "description": "NodeJS implementation of DHCP server",
  "main": "src/index.js",
  "directories": {
    "example": "examples"
  },
  "devDependencies": {
    "@types/mocha": "10.0.8",
    "mocha": "10.7.3",
    "ts-node": "10.9.2"
  },
  "overrides": {
    "braces": "3.0.3"
  },
  "scripts": {
    "test": "mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/microshine/dhcp-mon.git"
  },
  "keywords": [
    "dhcp",
    "net",
    "mac",
    "server",
    "client",
    "socket"
  ],
  "author": "mailsvb",
  "contributors": [
    "mailsvb@gmail.com",
    "Miroshin Stepan<microshine@mail.ru>",
    "Protasova Elena<lendjeina@gmail.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/microshine/dhcp-mon/issues"
  },
  "homepage": "https://github.com/microshine/dhcp-mon#readme",
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "reporter": [
      "text-summary",
      "html"
    ]
  }
}
