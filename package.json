{
  "name": "dhcp-mon",
  "version": "1.0.6",
  "description": "NodeJS implementation of DHCP socket connection",
  "main": "out/index.js",
  "module": "out/index.mjs",
  "types": "out/index.d.ts",
  "directories": {
    "example": "examples"
  },
  "dependencies": {
    "rollup-plugin-typescript2": "^0.36.0",
    "tslib": "^2.6.2"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.11.30",
    "coveralls": "^3.1.1",
    "mocha": "^10.4.0",
    "nyc": "^15.1.0",
    "rollup": "^4.13.1",
    "rollup-plugin-dts": "^6.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.3"
  },
  "scripts": {
    "prepare": "npm run build",
    "test": "mocha",
    "build": "rollup -c",
    "prepatch": "npm run build",
    "patch": "npm version patch",
    "pub": "npm run patch && npm publish && git push",
    "coverage": "nyc npm test",
    "coveralls": "nyc report --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/microshine/dhcp-mon.git"
  },
  "keywords": [
    "dhcp",
    "net",
    "mac",
    "server",
    "client",
    "socket"
  ],
  "author": "microshine",
  "contributors": [
    "Miroshin Stepan<microshine@mail.ru>",
    "Protasova Elena<lendjeina@gmail.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/microshine/dhcp-mon/issues"
  },
  "homepage": "https://github.com/microshine/dhcp-mon#readme",
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "reporter": [
      "text-summary",
      "html"
    ]
  }
}
